<% form_for @employee_pf, :url=>  employee_pf_path(@employee),:method=>:put do |form| %>
  <%= render :partial=>'form', :locals=>{:f=>form} %>
  <p class="submit">
    <%= form.submit t('button.update'), :class=>"btn" %> <%= "#{t('common.or')}" %>
    <%= link_to(t('button.cancel'), employee_pfs_path(@employee), :class => 'cancel') %>
  </p>
<% end %>
