.bottom {
    border-top:2px solid #DDDDDD;
    padding-bottom:5px;
    padding-left:28px;
    padding-top:10px;
    font-size:12px;
}

.card_name{
    font-size:14px;
    font-weight:bold;
    font-family:sans-serif;
    padding-top:10px;
    padding-bottom:15px;
}

.card_form{
    padding-top:10px;
    font-size:13px;
    font-weight:bold;
    color:gray;
}
div.salary_sheet_actions{
    padding-top:10px;
}

.error_message{
    padding-left:10px;
}

.graph_box {
    background-color:#F4F9FE;
    border:1px solid #B5CADF;
    float:left;
    height:220px;
    margin:9px;
    overflow:hidden;
    padding:0;
    width:386px;
}

.graph_box_right {
    background-color:#F4F9FE;
    border:1px solid #B5CADF;
    float:left;
    height:220px;
    margin:9px;
    overflow:hidden;
    padding:0;
    width:386px;
    float:right;
}

.graph {
    width: 100%;
    border: 1px solid #eee;

}

.graph .bar {
    display: block;
    position: relative;
    background: #B1D632;
    text-align: center;
    color: #333;
    height: 5em;
    line-height: 2em;
}

.rec_amount{
    color:#0fab1e;
}
.total_amount{
    color:red;
}
span.error {
    margin-left: 10px;
    background-color: #FF2244;
    color: #FFE900;
    font-size: 0.9em;
    padding: 2px 4px;
    font-weight: normal;
}

.home-vedio{
    text-align:center;
}


.import_employees{
    margin: 10px;
}

ul.salary_sheet_options > li > a {
    color:Gray;
    font-weight:normal;
}
/* Date Nav */
ul.date-nav {
    margin: 0;
}
ul.date-nav li {
    display: inline;
    list-style: none;
    margin: 0;
    float: left;
}
ul.date-nav li a {
    text-decoration: none;
    padding: 6px 5px;
    float: left;
}
ul.date-nav li a:hover {
    text-decoration: none;
    background-color: #eee;
}
ul.date-nav li.active a {
    background: #333;
    color: #fff;
    font-weight: bold;
}
.resend_email{
    padding:7px 4px !important;
    font-size: 10px;
}