/*
================================================================================
Self Clearing Floats
================================================================================
*/

#nav:after,
div.page-header:after,
#main:after,
div.group:after,
ul.form li:after,
.calculator_list .checklist li:after,
ul.month-nav:after,
#help-panel:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

/* trigger hasLayout and target ie5+6/win only */
* html #nav,
* html div.page-header,
* html #main,
* html div.group,
* html ul.form li,
* html .calculator_list .checklist li,
* html ul.month-nav,
* html #help-panel {
  height: 1%;
}

/* trigger hasLayout and target ie7/win only */
*:first-child+html #nav,
*:first-child+html div.page-header,
*:first-child+html #main,
*:first-child+html div.group,
*:first-child+html ul.form li,
*:first-child+html .calculator_list .checklist li,
*:first-child+html ul.month-nav,
*:first-child+html #help-panel {
  min-height: 1px;
}

